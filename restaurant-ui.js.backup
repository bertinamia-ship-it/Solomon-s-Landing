function getMenuData() {
    const lang = restaurantSystem.getCurrentLanguage();
    const priceAdjustment = restaurantSystem.getPriceAdjustment();
    
    const adjustPrice = (priceStr) => {
        // Handle price ranges like "$75-$95"
        if (priceStr.includes('-')) {
            const [min, max] = priceStr.split('-');
            const minPrice = parseFloat(min.replace('$', ''));
            const maxPrice = parseFloat(max.replace('$', ''));
            const adjMin = Math.round(minPrice * (1 + priceAdjustment / 100));
            const adjMax = Math.round(maxPrice * (1 + priceAdjustment / 100));
            return `$${adjMin}-$${adjMax}`;
        }
        const price = parseFloat(priceStr.replace('$', ''));
        if (isNaN(price)) return priceStr;
        const adjusted = price * (1 + priceAdjustment / 100);
        return '$' + Math.round(adjusted);
