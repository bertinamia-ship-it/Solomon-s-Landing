function closeTableConfirm(tableNumber) {
    const lang = restaurantSystem.getCurrentLanguage();
    const table = restaurantSystem.getTable(tableNumber);
    const message = lang === 'es' 
        ? `¬øCerrar Mesa #${tableNumber}?\n\nCliente: ${table.guestInfo?.lastName || 'N/A'}\nTotal: ${restaurantSystem.formatCurrency(table.total)}\n\nEsta acci√≥n no se puede deshacer.`
        : `Close Table #${tableNumber}?\n\nGuest: ${table.guestInfo?.lastName || 'N/A'}\nTotal: ${restaurantSystem.formatCurrency(table.total)}\n\nThis action cannot be undone.`;
    
    if (confirm(message)) {
        restaurantSystem.closeTable(tableNumber);
        loadWaiterInterface();
    }
}

