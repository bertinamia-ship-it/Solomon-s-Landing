// Polling interval for table closure detection
let tablePollingInterval = null;

function loadIPadInterface() {
    const user = restaurantSystem.getCurrentUser();
    const currentTable = sessionStorage.getItem('current_table');
    const lang = restaurantSystem.getCurrentLanguage();
    const t = (en, es) => lang === 'es' ? es : en;
    
    // Auto-assign table number based on iPad device (ipad1 = mesa 1, ipad2 = mesa 2, etc.)
    const autoTableNumber = user.username.replace('ipad', '');
    
    // Clear any existing polling
    if (tablePollingInterval) {
        clearInterval(tablePollingInterval);
        tablePollingInterval = null;
    }
    
    // If there's an active table, start polling to detect closure
    if (currentTable) {
        tablePollingInterval = setInterval(() => {
            const table = restaurantSystem.getTable(currentTable);
            // If table no longer exists, it was closed - return to home
            if (!table) {
                sessionStorage.removeItem('current_table');
                restaurantSystem.currentTable = null; // Clear system reference
                clearInterval(tablePollingInterval);
                tablePollingInterval = null;
                loadIPadInterface(); // Reload to show home screen
            }
        }, 2000); // Check every 2 seconds
    }
    
    const dashboard = document.getElementById('restaurantDashboard');
    dashboard.innerHTML = `
        <div class="dashboard-header" style="background: linear-gradient(135deg, #165b33, #1a7a42); padding: 1.5rem; display: none;">
