function startTableSession() {
    const lang = restaurantSystem.getCurrentLanguage();
    const tableNumber = document.getElementById('tableNumberInput').value;
    const lastName = document.getElementById('guestLastName').value.trim();
    const specialInfo = document.getElementById('specialInfo').value.trim();
    
    // Validaciones
    if (!tableNumber) {
        alert(lang === 'es' ? '‚ùå Por favor ingrese un n√∫mero de mesa' : '‚ùå Please enter a table number');
        return;
    }
    
    if (!lastName) {
        alert(lang === 'es' ? '‚ùå Por favor ingrese su apellido' : '‚ùå Please enter your last name');
        return;
    }
    
    // Informaci√≥n simplificada del comensal
    const guestInfo = {
        lastName,
        specialInfo: specialInfo || (lang === 'es' ? 'Ninguna' : 'None')
    };
    
    if (restaurantSystem.startTable(tableNumber, guestInfo)) {
        sessionStorage.setItem('current_table', tableNumber); // Ensure sessionStorage is set
        loadIPadInterface();
    } else {
        alert(lang === 'es' ? '‚ùå Esta mesa ya est√° activa. Por favor elija otro n√∫mero.' : '‚ùå This table is already active. Please choose another number.');
    }
}

